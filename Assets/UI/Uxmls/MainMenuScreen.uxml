            </ui:VisualElement>
            <ui:VisualElement name="menu_right" style="width: 50%; justify-content: center; align-items: center;">
                <ui:Label tabindex="-1" parse-escape-sequences="true" display-tooltip-when-elided="true" name="menu_graphic" style="background-image: resource(&apos;Images/211751_gear_icon&apos;); width: 100%; height: 80%;" />
