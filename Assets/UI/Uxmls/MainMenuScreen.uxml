                        <ui:Button text="More" parse-escape-sequences="true" display-tooltip-when-elided="true" name="menu_button-more" class="menu-button" />
                    </ui:VisualElement>
                    <ui:VisualElement name="menu_button-container--2" style="position: relative;">
