<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/Uss/MainMenuStyle.uss?fileID=7433441132597879392&amp;guid=f2a7515674d3261439babab637c5cc32&amp;type=3#MainMenuStyle" />
    <ui:VisualElement name="menu_container" style="flex-grow: 1; align-items: center; justify-content: center;">
        <ui:VisualElement name="menu_background" style="flex-grow: 1; position: absolute; width: 100%; height: 100%; top: 0; left: 3px;" />
        <ui:VisualElement name="menu_contents" style="width: 70%; height: 80%; flex-direction: row;">
            <ui:VisualElement name="menu_left" style="width: 50%; align-items: center; justify-content: space-around; padding-top: 10%; padding-bottom: 10%;">
                <ui:Label tabindex="-1" text="Sample Quiz" parse-escape-sequences="true" display-tooltip-when-elided="true" name="menu_title" style="-unity-font-definition: resource(&apos;Fonts/Roboto/Roboto-Black SDF&apos;); font-size: 70px; color: rgb(255, 135, 4);" />
                <ui:VisualElement name="menu_button-containers" style="align-items: center;">
                    <ui:VisualElement name="menu_button-container--1" style="position: relative;">
                        <ui:Button text="Play" parse-escape-sequences="true" display-tooltip-when-elided="true" name="menu_button-play" class="menu-button" />
